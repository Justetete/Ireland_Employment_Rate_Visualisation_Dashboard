{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://docs.google.com/spreadsheets/d/e/2PACX-1vSy0fjW_E2D_Nr6jUmwRAuDDaaVt2nVD9K1-6TSPVvnyNxFSyCWco0fzyWbBwOO_cMHDoCSrnkimWQx/pub?gid=159685356&single=true&output=csv",
        "format": {"type": "csv"}
    },
    "transform": [
        {"filter": "datum['NUTS 2 Region'] == 'Ireland'"},
        {"filter": "datum['Education Attainment Level'] == 'Levels  0-8'"},
        {"filter": "datum['Age Group'] == 'All ages'"},
        {"filter": "datum['Sex'] != 'Both sexes'"}
    ],
    "width": 600,
    "height": 300,
    "title": " Unemployment Rate by Gender between 2019 to 2024",
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "Year",
            "type": "nominal",
            "title": "Year",
            "axis": {"labelAngle":0}
        },
        "y": {
            "field": "Unemployment rate",
            "type": "quantitative",
            "scale": {"domain": [0, 8]},
            "axis": {"titleColor": "#1f77b4", "grid": false}
        },
        "color": {
            "field": "Sex",
            "type": "nominal",
            "title": "Gender",
            "scale": {"scheme": "category10"}
        },
        "xOffset": {"field": "Sex"}
    }
}