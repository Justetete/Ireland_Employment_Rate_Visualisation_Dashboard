{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://docs.google.com/spreadsheets/d/e/2PACX-1vRP9mr_HNIp8Zs5TttZybbpn1gGRxpD63t_qiiTIrz1FSpJby4rTZdWy1r1eUsYnFh4jn2haVCFxcUZ/pub?gid=741666015&single=true&output=csv",
        "format": {"type": "csv"}
    },
    "transform": [
        {
            "lookup": "Year",
            "from": {
                "data": {"url": "https://docs.google.com/spreadsheets/d/e/2PACX-1vSy0fjW_E2D_Nr6jUmwRAuDDaaVt2nVD9K1-6TSPVvnyNxFSyCWco0fzyWbBwOO_cMHDoCSrnkimWQx/pub?gid=159685356&single=true&output=csv"},
                "key": "Year",
                "fields": ["Annual unemployment rate"]
            }
        },

        {"filter": "datum['NUTS 2 Region'] == 'Ireland'"},
        {"filter": "datum['Education Attainment Level'] == 'Levels of Education (Levels  0-8)'"},
        {"filter": "datum['Age Group'] == 'All ages'"},
        {"filter": "datum['Sex'] != 'Both sexes'"},

        {
            "fold": ["Annual employment rate", "Annual unemployment rate"],
            "as": ["Rate Type", "Rate Value"]
        }
    ],
    "width": 600,
    "height": 400,
    "title": "Employment and Unemployment Rate by Gender (2019-2024)",
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "Year",
            "type": "ordinal",
            "title": "Year"
        },
        "y": {
            "field": "Rate Value",
            "type": "quantitative",
            "title": "Rate (%)",
            "scale": {"domain": [0, 100]}
        },
        "xOffset": {
            "field": "Sex"
        },
        "color": {
            "field": "Sex",
            "type": "nominal",
            "title": "Gender",
            "scale": {"scheme": "category10"}
        },
        "column": {
            "field": "Rate Type",
            "type": "nominal",
            "title": "Metric"
        }
    }
}